{
  "name": "project-name",
  "version": "1.0.0",
  "description": "The https://project-name.com web app",
  "keywords": [
    "react",
    "next.js",
    "web",
    "app",
    "www"
  ],
  "author": "David Cunha <davidgoncalvescunha@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "db:migrate": "sequelize --migrations-path=./server/migrations db:migrate",
    "db:migrate:undo": "sequelize --migrations-path=./server/migrations db:migrate:undo",
    "db:migrate:seeds": "sequelize --migrations-path=./server/migrations db:migrate && sequelize --seeders-path=./server/seeders db:seed:all",
    "dev": "cross-env NODE_ENV=development node index.js",
    "test": "cross-env NODE_ENV=test jest",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node index.js",
    "lint": "eslint --fix . && stylelint \"./client/**/*.js\"",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "bundle:analyzer": "cross-env ANALYZER=1 next build"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
