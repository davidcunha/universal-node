{
  "env": {
    "development": {
      "presets": ["next/babel"]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  },
  "plugins": [
    ["babel-plugin-root-import", { "rootPathSuffix": "client" }],
    ["transform-class-properties"]
  ]
}
